<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="NativeCamperVans.Views.HomePageDetail"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:context="clr-namespace:ContextMenu.Views;assembly=ContextMenu"
    xmlns:conv="clr-namespace:NativeCamperVans.Converters"
    xmlns:d="http://xamarin.com/schemas/2014/forms/design"
    xmlns:local="clr-namespace:NativeCamperVans.MarkupExtensions"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:pancake="clr-namespace:Xamarin.Forms.PancakeView;assembly=Xamarin.Forms.PancakeView"
    xmlns:renders="clr-namespace:NativeCamperVans.Renders"
    xmlns:viewModels="clr-namespace:NativeCamperVans.ViewModels"
    BackgroundColor="{StaticResource MaxVonWhite}"
    NavigationPage.HasNavigationBar="False"
    mc:Ignorable="d">
    <ContentPage.Resources>
        <ResourceDictionary>
            <conv:MenuFitWidthConverter x:Key="MenuFitWidthConverter" />
            <conv:IsMutedToTextConverter x:Key="IsMutedToTextConverter" />
            <conv:IsMutedToOpacityConverter x:Key="IsMutedToOpacityConverter" />
            <conv:IsMutedToBackgroundColorConverter x:Key="IsMutedToBackgroundColorConverter" />
        </ResourceDictionary>
    </ContentPage.Resources>

    <ContentPage.BindingContext>
        <viewModels:BaseItemsViewModel />
    </ContentPage.BindingContext>

    <ScrollView>

        <Grid Margin="0,30,0,0" VerticalOptions="FillAndExpand">
            <Grid.RowDefinitions>
                <RowDefinition Height="50" />
                <RowDefinition Height="60" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>

            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="80" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="50" />
                    <ColumnDefinition Width="50" />
                </Grid.ColumnDefinitions>
                <Grid.HeightRequest>
                    <OnPlatform x:TypeArguments="x:Double">
                        <On Platform="iOS">
                            <OnIdiom
                            x:TypeArguments="x:Double"
                            Phone="50"
                            Tablet="60" />
                        </On>
                        <On Platform="Android,Windows">
                            <OnIdiom
                            x:TypeArguments="x:Double"
                            Phone="50"
                            Tablet="60" />
                        </On>
                    </OnPlatform>
                </Grid.HeightRequest>
                <ImageButton
                x:Name="btnMenu"
                Margin="0"
                Padding="0"
                BackgroundColor="Transparent"
                Clicked="btnMenu_Clicked"
                HeightRequest="25"
                Source="{local:EmbeddedImage ResourceId=NativeCamperVans.Assets.iconMenu.png}"
                VerticalOptions="CenterAndExpand" />

                <Label
                Grid.Column="1"
                Margin="0,0,0,0"
                FontAttributes="Bold"
                FontFamily="Roboto-Bold"
                FontSize="Large"
                HorizontalOptions="CenterAndExpand"
                Text="Home"
                TextColor="{StaticResource MaxVonRed}"
                VerticalOptions="CenterAndExpand" />

                <ImageButton
                Grid.Column="2"
                BackgroundColor="Transparent"
                HeightRequest="30"
                IsVisible="false"
                Source="{local:EmbeddedImage ResourceId=NativeCamperVans.Assets.iconRedSearch.png}"
                VerticalOptions="CenterAndExpand" />

                <ImageButton
                Grid.Column="3"
                BackgroundColor="Transparent"
                HeightRequest="30"
                IsVisible="false"
                Source="{local:EmbeddedImage ResourceId=NativeCamperVans.Assets.iconRedFilter.png}"
                VerticalOptions="CenterAndExpand" />
            </Grid>

            <Grid
            Grid.Row="1"
            Margin="10,10,15,0"
            Padding="0"
            BackgroundColor="{StaticResource MaxVonLightGray}"
            ColumnSpacing="0">
                <Grid.HeightRequest>
                    <OnPlatform x:TypeArguments="x:Double">
                        <On Platform="iOS">
                            <OnIdiom
                            x:TypeArguments="x:Double"
                            Phone="60"
                            Tablet="60" />
                        </On>
                        <On Platform="Android,Windows">
                            <OnIdiom
                            x:TypeArguments="x:Double"
                            Phone="60"
                            Tablet="60" />
                        </On>
                    </OnPlatform>
                </Grid.HeightRequest>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Button
                x:Name="btnMyRentals"
                Grid.Column="0"
                BackgroundColor="{StaticResource MaxVonRed}"
                Clicked="btnMyRentals_Clicked"
                CornerRadius="10"
                FontFamily="Roboto-Medium"
                HorizontalOptions="FillAndExpand"
                Text="Active Rentals"
                TextColor="#ffffff">
                    <Button.HeightRequest>
                        <OnPlatform x:TypeArguments="x:Double">
                            <On Platform="iOS">
                                <OnIdiom
                                x:TypeArguments="x:Double"
                                Phone="50"
                                Tablet="60" />
                            </On>
                            <On Platform="Android,Windows">
                                <OnIdiom
                                x:TypeArguments="x:Double"
                                Phone="50"
                                Tablet="60" />
                            </On>
                        </OnPlatform>
                    </Button.HeightRequest>
                    <Button.FontSize>
                        <OnPlatform x:TypeArguments="x:Double">
                            <On Platform="iOS">
                                <OnIdiom
                                x:TypeArguments="x:Double"
                                Phone="14"
                                Tablet="18" />
                            </On>
                            <On Platform="Android,Windows">
                                <OnIdiom
                                x:TypeArguments="x:Double"
                                Phone="16"
                                Tablet="20" />
                            </On>
                        </OnPlatform>
                    </Button.FontSize>
                </Button>
                <Button
                x:Name="btnPastRental"
                Grid.Column="1"
                Margin="0,0,-5,0"
                BackgroundColor="{StaticResource MaxVonLightGray}"
                Clicked="btnPastRental_Clicked"
                CornerRadius="10"
                FontFamily="Roboto-Medium"
                HorizontalOptions="FillAndExpand"
                Text="Past Rentals"
                TextColor="Black">
                    <Button.HeightRequest>
                        <OnPlatform x:TypeArguments="x:Double">
                            <On Platform="iOS">
                                <OnIdiom
                                x:TypeArguments="x:Double"
                                Phone="50"
                                Tablet="60" />
                            </On>
                            <On Platform="Android,Windows">
                                <OnIdiom
                                x:TypeArguments="x:Double"
                                Phone="50"
                                Tablet="60" />
                            </On>
                        </OnPlatform>
                    </Button.HeightRequest>
                    <Button.FontSize>
                        <OnPlatform x:TypeArguments="x:Double">
                            <On Platform="iOS">
                                <OnIdiom
                                x:TypeArguments="x:Double"
                                Phone="14"
                                Tablet="18" />
                            </On>
                            <On Platform="Android,Windows">
                                <OnIdiom
                                x:TypeArguments="x:Double"
                                Phone="16"
                                Tablet="20" />
                            </On>
                        </OnPlatform>
                    </Button.FontSize>
                </Button>
            </Grid>

            <StackLayout
            x:Name="grdRentals"
            Grid.Row="2"
            Margin="10,0"
            VerticalOptions="Fill">
                <ListView
                x:Name="upcomingReservation"
                Margin="0,50,0,0"
                BackgroundColor="Transparent"
                HorizontalScrollBarVisibility="Never"
                ItemSelected="UpcomingReservation_ItemSelected"
                RowHeight="330"
                SeparatorVisibility="None"
                VerticalOptions="Center"
                VerticalScrollBarVisibility="Never">
                    <ListView.RowHeight>
                        <OnPlatform x:TypeArguments="x:Int32">
                            <On Platform="iOS">
                                <OnIdiom
                                x:TypeArguments="x:Int32"
                                Phone="340"
                                Tablet="390" />
                            </On>
                            <On Platform="Android,Windows">
                                <OnIdiom
                                x:TypeArguments="x:Int32"
                                Phone="340"
                                Tablet="390" />
                            </On>
                        </OnPlatform>

                    </ListView.RowHeight>
                    <ListView.ItemTemplate>
                        <DataTemplate>
                            <ViewCell>
                                <Frame
                                    BackgroundColor="White"
                                Margin="10,10"
                                Padding="0"
                                BorderColor="LightGray"
                                CornerRadius="10"
                                HasShadow="False"
                                HeightRequest="230"
                                VerticalOptions="FillAndExpand">
                                    <Grid Margin="5,10" Padding="0">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="45" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="3*" />
                                            <ColumnDefinition Width="7*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid
                                        Grid.Row="0"
                                        Grid.ColumnSpan="2"
                                        Margin="15,0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="Auto" />
                                            </Grid.ColumnDefinitions>

                                            <StackLayout
                                            Margin="0"
                                            Padding="0"
                                            Orientation="Horizontal"
                                            VerticalOptions="CenterAndExpand">
                                                <Label
                                                FontFamily="Roboto-Bold"
                                                Text="Reservation Number : "
                                                TextColor="{StaticResource Black}">
                                                    <Label.FontSize>
                                                        <OnPlatform x:TypeArguments="x:Double">
                                                            <On Platform="iOS">
                                                                <OnIdiom
                                                                x:TypeArguments="x:Double"
                                                                Phone="16"
                                                                Tablet="20" />
                                                            </On>
                                                            <On Platform="Android,Windows">
                                                                <OnIdiom
                                                                x:TypeArguments="x:Double"
                                                                Phone="14"
                                                                Tablet="18" />
                                                            </On>
                                                        </OnPlatform>
                                                    </Label.FontSize>
                                                </Label>
                                                <Label
                                                FontFamily="Roboto-Bold"
                                                Text="{Binding reservationData.Reservationview.ReservationNumber}"
                                                TextColor="{StaticResource Black}"
                                                VerticalOptions="Center">
                                                    <Label.FontSize>
                                                        <OnPlatform x:TypeArguments="x:Double">
                                                            <On Platform="iOS">
                                                                <OnIdiom
                                                                x:TypeArguments="x:Double"
                                                                Phone="16"
                                                                Tablet="20" />
                                                            </On>
                                                            <On Platform="Android,Windows">
                                                                <OnIdiom
                                                                x:TypeArguments="x:Double"
                                                                Phone="14"
                                                                Tablet="18" />
                                                            </On>
                                                        </OnPlatform>
                                                    </Label.FontSize>
                                                </Label>
                                            </StackLayout>

                                            <Button
                                            Grid.Column="1"
                                            Margin="0"
                                            Padding="10,0"
                                            BackgroundColor="Transparent"
                                            BorderColor="{Binding reservationData.Reservationview.Status, Converter={StaticResource ReservationStatusToColor}}"
                                            BorderWidth="2"
                                            CornerRadius="10"
                                            FontFamily="Roboto-Medium"
                                            HeightRequest="30"
                                            Text="{Binding reservationData.Reservationview.PageTitle}"
                                            TextColor="{Binding reservationData.Reservationview.Status, Converter={StaticResource ReservationStatusToColor}}"
                                            VerticalOptions="CenterAndExpand">
                                                <Button.FontSize>
                                                    <OnPlatform x:TypeArguments="x:Double">
                                                        <On Platform="iOS">
                                                            <OnIdiom
                                                            x:TypeArguments="x:Double"
                                                            Phone="13"
                                                            Tablet="16" />
                                                        </On>
                                                        <On Platform="Android,Windows">
                                                            <OnIdiom
                                                            x:TypeArguments="x:Double"
                                                            Phone="14"
                                                            Tablet="18" />
                                                        </On>
                                                    </OnPlatform>
                                                </Button.FontSize>
                                            </Button>
                                        </Grid>
                                        <BoxView
                                        Grid.Row="0"
                                        Grid.ColumnSpan="2"
                                        HeightRequest="1"
                                        HorizontalOptions="FillAndExpand"
                                        VerticalOptions="EndAndExpand"
                                        Color="{StaticResource MaxVonGray}" />

                                        <Grid
                                        Grid.Row="1"
                                        Grid.Column="0"
                                        Grid.ColumnSpan="2"
                                        Margin="15,0">
                                            <StackLayout
                                            Margin="0"
                                            Padding="0"
                                            IsVisible="{Binding isTimerVisible}"
                                            Orientation="Horizontal"
                                            VerticalOptions="CenterAndExpand">
                                                <Label
                                                FontFamily="Roboto-Bold"
                                                IsVisible="{Binding isTimerVisible}"
                                                Text="Return in : "
                                                TextColor="{StaticResource Black}">
                                                    <Label.FontSize>
                                                        <OnPlatform x:TypeArguments="x:Double">
                                                            <On Platform="iOS">
                                                                <OnIdiom
                                                                x:TypeArguments="x:Double"
                                                                Phone="16"
                                                                Tablet="20" />
                                                            </On>
                                                            <On Platform="Android,Windows">
                                                                <OnIdiom
                                                                x:TypeArguments="x:Double"
                                                                Phone="18"
                                                                Tablet="22" />
                                                            </On>
                                                        </OnPlatform>
                                                    </Label.FontSize>
                                                </Label>
                                                <Label
                                                FontFamily="Roboto-Bold"
                                                IsVisible="{Binding isTimerVisible}"
                                                Text="01:59:52"
                                                TextColor="{StaticResource MaxVonRed}">
                                                    <Label.FontSize>
                                                        <OnPlatform x:TypeArguments="x:Double">
                                                            <On Platform="iOS">
                                                                <OnIdiom
                                                                x:TypeArguments="x:Double"
                                                                Phone="16"
                                                                Tablet="20" />
                                                            </On>
                                                            <On Platform="Android,Windows">
                                                                <OnIdiom
                                                                x:TypeArguments="x:Double"
                                                                Phone="18"
                                                                Tablet="22" />
                                                            </On>
                                                        </OnPlatform>
                                                    </Label.FontSize>
                                                </Label>
                                            </StackLayout>
                                        </Grid>

                                        <Grid
                                        Grid.Row="2"
                                        Grid.Column="0"
                                        Grid.ColumnSpan="2"
                                        Margin="15,0"
                                        ColumnSpacing="10"
                                        HeightRequest="120"
                                        RowSpacing="0"
                                        VerticalOptions="FillAndExpand">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="100" />
                                                <ColumnDefinition Width="*" />
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />
                                            </Grid.RowDefinitions>
                                            <Image
                                            Grid.Row="0"
                                            Grid.RowSpan="3"
                                            Aspect="AspectFit"
                                            Source="{Binding vehicleTypeModel.ImageUrl}" />

                                            <Label
                                            Grid.Column="1"
                                            Margin="0,0,0,5"
                                            FontAttributes="Bold"
                                            FontFamily="Roboto-Bold"
                                            Text="{Binding vehicleTypeModel.Sample}"
                                            TextColor="{StaticResource MaxVonRed}">
                                                <Label.FontSize>
                                                    <OnPlatform x:TypeArguments="x:Double">
                                                        <On Platform="iOS">
                                                            <OnIdiom
                                                            x:TypeArguments="x:Double"
                                                            Phone="16"
                                                            Tablet="20" />
                                                        </On>
                                                        <On Platform="Android,Windows">
                                                            <OnIdiom
                                                            x:TypeArguments="x:Double"
                                                            Phone="18"
                                                            Tablet="22" />
                                                        </On>
                                                    </OnPlatform>
                                                </Label.FontSize>
                                            </Label>

                                            <StackLayout
                                            Grid.Row="1"
                                            Grid.Column="1"
                                            Margin="0"
                                            Padding="0"
                                            Orientation="Horizontal">
                                                <Label
                                                Margin="0,0,0,5"
                                                FontFamily="Roboto-Medium"
                                                FontSize="Small"
                                                Text="Vehicle Type: "
                                                TextColor="{StaticResource MaxVonBlack}">
                                                    <Label.FontSize>
                                                        <OnPlatform x:TypeArguments="x:Double">
                                                            <On Platform="iOS">
                                                                <OnIdiom
                                                                x:TypeArguments="x:Double"
                                                                Phone="13"
                                                                Tablet="16" />
                                                            </On>
                                                            <On Platform="Android,Windows">
                                                                <OnIdiom
                                                                x:TypeArguments="x:Double"
                                                                Phone="14"
                                                                Tablet="18" />
                                                            </On>
                                                        </OnPlatform>
                                                    </Label.FontSize>
                                                </Label>
                                                <Label
                                                x:Name="VehicleType"
                                                Margin="0,0,0,5"
                                                FontFamily="Roboto-Medium"
                                                Text="{Binding vehicleTypeModel.VehicleTypeName}"
                                                TextColor="{StaticResource MaxVonBlack}">
                                                    <Label.FontSize>
                                                        <OnPlatform x:TypeArguments="x:Double">
                                                            <On Platform="iOS">
                                                                <OnIdiom
                                                                x:TypeArguments="x:Double"
                                                                Phone="13"
                                                                Tablet="16" />
                                                            </On>
                                                            <On Platform="Android,Windows">
                                                                <OnIdiom
                                                                x:TypeArguments="x:Double"
                                                                Phone="14"
                                                                Tablet="18" />
                                                            </On>
                                                        </OnPlatform>
                                                    </Label.FontSize>
                                                </Label>
                                            </StackLayout>
                                            <StackLayout
                                            Grid.Row="2"
                                            Grid.Column="1"
                                            Margin="0"
                                            Padding="0"
                                            Orientation="Horizontal"
                                            Spacing="15">
                                                <StackLayout
                                                Grid.Column="0"
                                                Margin="0"
                                                Padding="0"
                                                Orientation="Horizontal"
                                                Spacing="5">
                                                    <Image
                                                    BackgroundColor="Transparent"
                                                    HorizontalOptions="Center"
                                                    Source="{local:EmbeddedImage ResourceId=NativeCamperVans.Assets.iconGraySeat.png}"
                                                    VerticalOptions="CenterAndExpand">
                                                        <Image.HeightRequest>
                                                            <OnPlatform x:TypeArguments="x:Double">
                                                                <On Platform="iOS">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="10"
                                                                    Tablet="20" />
                                                                </On>
                                                                <On Platform="Android,Windows">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="10"
                                                                    Tablet="20" />
                                                                </On>
                                                            </OnPlatform>
                                                        </Image.HeightRequest>
                                                        <Image.WidthRequest>
                                                            <OnPlatform x:TypeArguments="x:Double">
                                                                <On Platform="iOS">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="10"
                                                                    Tablet="20" />
                                                                </On>
                                                                <On Platform="Android,Windows">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="10"
                                                                    Tablet="20" />
                                                                </On>
                                                            </OnPlatform>
                                                        </Image.WidthRequest>
                                                    </Image>
                                                    <Label
                                                    FontFamily="Roboto-Medium"
                                                    Text="{Binding vehicleTypeModel.Seats}"
                                                    TextColor="{StaticResource MaxVonBlack}"
                                                    VerticalOptions="CenterAndExpand">
                                                        <Label.FontSize>
                                                            <OnPlatform x:TypeArguments="x:Double">
                                                                <On Platform="iOS">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="12"
                                                                    Tablet="16" />
                                                                </On>
                                                                <On Platform="Android,Windows">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="12"
                                                                    Tablet="18" />
                                                                </On>
                                                            </OnPlatform>
                                                        </Label.FontSize>
                                                    </Label>
                                                </StackLayout>
                                                <StackLayout
                                                Margin="0"
                                                Padding="0"
                                                Orientation="Horizontal"
                                                Spacing="5">
                                                    <Image
                                                    BackgroundColor="Transparent"
                                                    HorizontalOptions="Center"
                                                    Source="{local:EmbeddedImage ResourceId=NativeCamperVans.Assets.iconGrayBag.png}"
                                                    VerticalOptions="CenterAndExpand">
                                                        <Image.HeightRequest>
                                                            <OnPlatform x:TypeArguments="x:Double">
                                                                <On Platform="iOS">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="10"
                                                                    Tablet="20" />
                                                                </On>
                                                                <On Platform="Android,Windows">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="10"
                                                                    Tablet="20" />
                                                                </On>
                                                            </OnPlatform>
                                                        </Image.HeightRequest>
                                                        <Image.WidthRequest>
                                                            <OnPlatform x:TypeArguments="x:Double">
                                                                <On Platform="iOS">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="10"
                                                                    Tablet="20" />
                                                                </On>
                                                                <On Platform="Android,Windows">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="10"
                                                                    Tablet="20" />
                                                                </On>
                                                            </OnPlatform>
                                                        </Image.WidthRequest>
                                                    </Image>
                                                    <Label
                                                    FontFamily="Roboto-Medium"
                                                    Text="{Binding vehicleTypeModel.Baggages}"
                                                    TextColor="{StaticResource MaxVonBlack}"
                                                    VerticalOptions="CenterAndExpand">
                                                        <Label.FontSize>
                                                            <OnPlatform x:TypeArguments="x:Double">
                                                                <On Platform="iOS">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="12"
                                                                    Tablet="16" />
                                                                </On>
                                                                <On Platform="Android,Windows">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="12"
                                                                    Tablet="18" />
                                                                </On>
                                                            </OnPlatform>
                                                        </Label.FontSize>
                                                    </Label>
                                                </StackLayout>
                                                <StackLayout
                                                Margin="0"
                                                Padding="0"
                                                Orientation="Horizontal"
                                                Spacing="5">
                                                    <Image
                                                    BackgroundColor="Transparent"
                                                    HorizontalOptions="Center"
                                                    Source="{local:EmbeddedImage ResourceId=NativeCamperVans.Assets.iconGrayFeature.png}"
                                                    VerticalOptions="CenterAndExpand">
                                                        <Image.HeightRequest>
                                                            <OnPlatform x:TypeArguments="x:Double">
                                                                <On Platform="iOS">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="10"
                                                                    Tablet="20" />
                                                                </On>
                                                                <On Platform="Android,Windows">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="10"
                                                                    Tablet="20" />
                                                                </On>
                                                            </OnPlatform>
                                                        </Image.HeightRequest>
                                                        <Image.WidthRequest>
                                                            <OnPlatform x:TypeArguments="x:Double">
                                                                <On Platform="iOS">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="10"
                                                                    Tablet="20" />
                                                                </On>
                                                                <On Platform="Android,Windows">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="10"
                                                                    Tablet="20" />
                                                                </On>
                                                            </OnPlatform>
                                                        </Image.WidthRequest>
                                                    </Image>
                                                    <Label
                                                    FontFamily="Roboto-Medium"
                                                    Text="{Binding vehicleTypeModel.Transmission}"
                                                    TextColor="{StaticResource MaxVonBlack}"
                                                    VerticalOptions="CenterAndExpand">
                                                        <Label.FontSize>
                                                            <OnPlatform x:TypeArguments="x:Double">
                                                                <On Platform="iOS">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="12"
                                                                    Tablet="16" />
                                                                </On>
                                                                <On Platform="Android,Windows">
                                                                    <OnIdiom
                                                                    x:TypeArguments="x:Double"
                                                                    Phone="12"
                                                                    Tablet="18" />
                                                                </On>
                                                            </OnPlatform>
                                                        </Label.FontSize>
                                                    </Label>
                                                </StackLayout>
                                            </StackLayout>

                                            <Label
                                            Grid.Row="3"
                                            Grid.Column="1"
                                            Margin="0,0,0,5"
                                            FontAttributes="Bold"
                                            FontFamily="Roboto-Bold"
                                            TextColor="{StaticResource MaxVonRed}">
                                                <Label.FormattedText>
                                                    <FormattedString>
                                                        <Span
                                                        FontAttributes="Bold"
                                                        FontFamily="Roboto-Bold"
                                                        Text="Days: "
                                                        TextColor="{StaticResource MaxVonRed}">
                                                            <Span.FontSize>
                                                                <OnPlatform x:TypeArguments="x:Double">
                                                                    <On Platform="iOS">
                                                                        <OnIdiom
                                                                        x:TypeArguments="x:Double"
                                                                        Phone="13"
                                                                        Tablet="16" />
                                                                    </On>
                                                                    <On Platform="Android,Windows">
                                                                        <OnIdiom
                                                                        x:TypeArguments="x:Double"
                                                                        Phone="14"
                                                                        Tablet="18" />
                                                                    </On>
                                                                </OnPlatform>
                                                            </Span.FontSize>
                                                        </Span>
                                                        <Span
                                                        FontAttributes="Bold"
                                                        FontFamily="Roboto-Bold"
                                                        Text="{Binding reservationData.Reservationview.TotalDays}"
                                                        TextColor="{StaticResource MaxVonRed}">
                                                            <Span.FontSize>
                                                                <OnPlatform x:TypeArguments="x:Double">
                                                                    <On Platform="iOS">
                                                                        <OnIdiom
                                                                        x:TypeArguments="x:Double"
